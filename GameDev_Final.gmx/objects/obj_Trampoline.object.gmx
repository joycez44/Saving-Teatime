            <string>//Pushing

if (place_meeting(x, y, obj_Player) &amp;&amp; obj_Player.vspd == 0) {
    hspd = obj_Player.hspd;
}
else {
    hspd = 0;
}

//Gravity
if (vspd &lt;= vspd_cap) 
{
    vspd += grv;
    
    
    grv += .02;
} 

//Check collisions
if (place_meeting(x + hspd, y, obj_Floor)) 
{
    while(!place_meeting(x + sign(hspd), y, obj_Floor))
    {
        x += sign(hspd);
    }
    hspd = 0;
}

if ( x + hspd &lt; room_width - sprite_width/2  &amp;&amp; x + hspd &gt; sprite_width/2) {
    x += hspd;
}


if (place_meeting(x, y + vspd, obj_Floor)) 
{
    while (!place_meeting(x, y + sign(vspd), obj_Floor)) 
    {
        y += sign(vspd);
    }
    vspd = 0;
    grv = init_grv;
}

y += vspd;
</string>
